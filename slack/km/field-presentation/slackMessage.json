  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        },
        {
          "id": "slack_slackChannel"
        },
        {
          "id": "slack_messageText"
        },
        {
          "id": "slack_messageTimestamp"
        },
        {
          "id": "slack_messageURL"
        },
        {
          "id": "slack_reactions"
        },
        {
          "id": "slack_isPinned"
        },
        {
          "id": "slack_messages"
        }
      ]
    },
    {
      "displayName": "User Metadata",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "slack_linkedUser"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        }
      ]
    }
  ]
